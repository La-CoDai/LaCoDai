{% extends 'home-admin.html.twig' %}
{% block body %}
<div class="container">
    <div class="row title-admin"><h1 class="m-0">ADD BRAND</h1></div>
    <div class="row content-admin">
        {{ form_start(form) }}
            <div class="my-2">
                {{ form_widget(form.bname) }}
            </div>
            <div>
                {{ form_widget(form.add)}}
                <input type="reset" value="RESET" class="btn btn-primary">
                <a href="{{path('show_brand')}}"><input type="button" value="BRAND LIST" class="btn btn-primary"></a>
            </div>
        {{ form_end(form) }}
    </div>
</div>
{% endblock %}